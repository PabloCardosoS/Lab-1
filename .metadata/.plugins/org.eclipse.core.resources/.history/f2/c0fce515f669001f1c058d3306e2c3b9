package whilelista14;

import java.util.Scanner;

public class Ex014 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int n = scanner.nextInt();
		int i = 1;
		while (i < (n - 1)) {
			i++;
			if (n % i == 0) {
				System.out.println("Não é primo.");
				break;
			}
						
		}
		System.out.println("É PRIMO.");
	}

}
