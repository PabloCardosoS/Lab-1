package whilelista5;

import java.util.Scanner;

public class Ex005 {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);

		int i = 0;
		int num = 1;
		while (i < 10) {
			String time = scanner.nextLine();
			System.out.print("Escreva o time: ");
			i++;
			if (time == "gremio") {
				num += num;
		}
		System.out.println(num);

	}

}
